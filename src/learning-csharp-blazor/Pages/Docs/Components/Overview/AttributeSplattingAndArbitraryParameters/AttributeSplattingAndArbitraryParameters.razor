<AttributeSplattingAndArbitraryParameters01 X="1" Y="2" Z="3"></AttributeSplattingAndArbitraryParameters01>

<!-- 使用 @@attributes 一次性传递多个属性  -->
<AttributeSplattingAndArbitraryParameters02
  X="3"
  @attributes="@(new Dictionary<string, object> { { "X", "1" }, { "Y", "2" }, { "Z", "3" } })">
</AttributeSplattingAndArbitraryParameters02>

<!-- 如果 @@attributes 中的某个参数跟其他参数同名, 则后面的会覆盖前面的 -->
<AttributeSplattingAndArbitraryParameters02
  @attributes="@(new Dictionary<string, object> { { "X", "1" }, { "Y", "2" }, { "Z", "3" } })"
  X="3">
</AttributeSplattingAndArbitraryParameters02>
